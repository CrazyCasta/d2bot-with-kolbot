// Blank starter used for testin

const D2BOT_JOIN = 1;
const D2BOT_GAMEINFO = 2;
const D2BOT_REQUESTGAME = 3;

var isUp = "no";

function main() {
	include("json2.js");
	include("OOG.js");

	if (!FileTools.exists("data/" + me.profile + ".json")) {
		DataFile.create();
	}
	
	while (true) {
		if (me.ingame) {
			if (isUp === "no") {
				isUp = "yes";
			}
		} else {
			isUp = "no";
		}
		
		delay(1000);
	}
}
